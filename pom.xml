  <packaging>jar</packaging>
   
	<build>
   <plugins>
     <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-surefire-plugin</artifactId>
       <version>2.10</version>
       <configuration>
         <includes>
           <include>**/*testHomePage.java</include>
         </includes>
         <shipTests>true</shipTests>
       </configuration>
     </plugin>
   </plugins>
 </build>

